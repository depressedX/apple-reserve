{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "minimum_chrome_version": "62.0",
  "name": "Autofill",
  "version": "9.6.1",
  "default_locale": "en",
  "description": "__MSG_generalDescription__",
  "icons": {
    "16": "images/icon16.png",
    "24": "images/icon24.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "96": "images/icon96.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": ["js/eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon16-off.png",
      "24": "images/icon24-off.png",
      "32": "images/icon32-off.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/autofill.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": false,
    "open_in_tab": true
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "file://*/*",
    "contextMenus",
    "storage",
    "unlimitedStorage",
    "webNavigation"
  ],
  "web_accessible_resources": [
    "images/icon24.png",
    "sounds/elric.ogg",
    "sounds/elric-err.ogg",
    "sounds/jeannie.ogg",
    "sounds/jeannie-err.ogg",
    "sounds/merlin.ogg",
    "sounds/merlin-err.ogg",
    "sounds/sabrina.ogg",
    "sounds/sabrina-err.ogg",
    "sounds/samantha.ogg",
    "sounds/samantha-err.ogg",
    "sounds/simon.ogg",
    "sounds/simon-err.ogg",
    "sounds/sound.ogg"
  ]
}
