{
  "name": "Apple预约辅助",
  "version": "1.0",
  "description": "啊啊啊啊啊啊",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "favicon.svg",
    "32": "favicon.svg",
    "48": "favicon.svg",
    "128": "favicon.svg"
  },
  "content_scripts": [
    {
      "matches": [
        "https://reserve-prime.apple.com/CN/zh_CN/reserve/F*",
        "https://reserve-prime.apple.com/CN/zh_CN/reserve/A*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "vue.js",
        "inject.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "favicon.svg",
    "default_title": "stock helper"
  },
  "manifest_version": 2
}
